{
  "manifest_version": 2,
  "name": "COP - Detran",
  "description": "Pega do COP e faz a busca da placa no Detran-RN",
  "version": "0.1",
  "permissions": [
    "activeTab"
	],
	"content_scripts": [
		{
			"matches": ["*://*.br/*"],
			"js": ["post.js"]
		}
	],
	
	"browser_action": {
 		"default_icon": "carro.png",
 		"default_popup": "Detran.html",
 		"default_title": "Detran-RN"
	}
}