{
  "manifest_version": 2,
  "name": "SICOP",
  "description": "Pega do SICOP e faz a busca da placa no Detran-RN",
  "version": "0.1",
  "permissions": ["activeTab"],
  "content_scripts": [
	{
		"matches": [ "<all_urls>" ],
        "all_frames": true,
		"js": ["jquery-3.4.1.min.js", "content.js"]            
	}
  ],
	
  "browser_action": {
 	"default_icon": "carro.png"
  },
	
	"background": {
		"scripts": ["background.js"],
        "persistent": false
	}
}